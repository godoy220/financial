{
  "development": {
    "use_env_variable": "DEV_DATABASE_URL",
    "dialect": "postgres",
    "port": 5432
  },
  "test": {
    "use_env_variable": "TEST_DATABASE_URL",
    "dialect": "postgres"
  },
  "production": {
    "use_env_variable": "DATABASE_URL",
    "dialect": "postgres",
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    }
  }
}